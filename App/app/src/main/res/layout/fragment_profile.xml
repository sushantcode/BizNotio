<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/h"
        tools:context="Fragments.ProfileFragment">


  <ImageView
          android:id="@+id/imageView"
          android:layout_width="114dp"
          android:layout_height="114dp"
          android:layout_marginStart="4dp"
          android:layout_marginTop="44dp"
          android:layout_marginEnd="4dp"
          android:layout_marginBottom="501dp"
          android:src="@drawable/profile"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintEnd_toEndOf="parent"

          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent"
          android:clickable="true"
          app:layout_constraintHorizontal_bias="0.235" app:layout_constraintVertical_bias="0.43"/>

  <TextView
          android:text="NAME"
          android:layout_width="141dp"
          android:textStyle="bold"
          android:layout_height="37dp" android:id="@+id/textView"
          app:layout_constraintBottom_toBottomOf="parent"
          android:textColor="#0C0C0C"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.29"
          app:layout_constraintVertical_bias="0.303" android:textSize="16sp"/>
  <ImageButton
          android:id="@+id/imagelogoutbutton"
          android:background="#0B0A0A"

          android:src="@drawable/tinys" tools:ignore="MissingConstraints" android:layout_height="32dp"
          android:layout_width="41dp"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintVertical_bias="0.947"/>

  <Button
          android:text="Edit Profile"

          android:layout_width="137dp"
          android:layout_height="50dp" android:id="@+id/edit_button"
          app:layout_constraintBottom_toBottomOf="@+id/imagelogoutbutton"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintVertical_bias="0.238" app:layout_constraintHorizontal_bias="0.84"/>
  <TextView
          android:text="Interests"
          android:layout_width="233dp"
          android:layout_height="69dp" android:id="@+id/Interests"
          android:textColor="#0C0C0C"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintVertical_bias="0.68" app:layout_constraintHorizontal_bias="0.718"/>
  <TextView
          android:text="Profession"
          android:layout_width="253dp"
          android:layout_height="47dp" android:id="@+id/profession"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.82"
          app:layout_constraintVertical_bias="0.45" android:textColor="#0C0C0C"/>
  <TextView
          android:text="Education"
          android:layout_width="252dp"
          android:layout_height="57dp" android:id="@+id/Education"
          android:textColor="#0C0C0C"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintVertical_bias="0.551" app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintHorizontal_bias="0.81"/>
  <TextView
          android:text="BizNotio Goals"
          android:layout_width="272dp"
          android:layout_height="61dp" android:id="@+id/Goals"
          android:layout_marginTop="26dp"
          android:textColor="#0C0C0C"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintVertical_bias="0.79" app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintHorizontal_bias="0.88"/>
  <de.hdodenhof.circleimageview.CircleImageView
          android:id="@+id/circle_image_profile"
          android:layout_width="114dp"
          android:layout_height="114dp"
          android:layout_marginEnd="8dp"
          app:layout_constraintBottom_toBottomOf="@+id/profile_image_clickable"
          app:layout_constraintEnd_toEndOf="@+id/imageView"
          app:layout_constraintStart_toStartOf="@+id/profile_image_clickable"
          app:layout_constraintTop_toTopOf="@+id/imageView" android:layout_marginTop="4dp"
          app:layout_constraintVertical_bias="0.19999999"/>
  <TextView
          android:text="0"
          android:textColor="#0C0C0C"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" android:id="@+id/numberofconnections"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.735"
          app:layout_constraintVertical_bias="0.123"/>


  <TextView
          android:text="Connections"
          android:layout_width="108dp"
          android:layout_height="35dp" android:id="@+id/connection"
          app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintHorizontal_bias="0.813" app:layout_constraintVertical_bias="0.179"
          android:textColor="#0E0F15" android:textSize="18sp"/>
  <Button
          android:text="Rate"
          android:layout_width="78dp"
          android:layout_height="37dp" android:id="@+id/Rating"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintHorizontal_bias="0.784" app:layout_constraintVertical_bias="0.298"/>
  <TextView
          android:text="Profession:"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" android:id="@+id/textView3"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
          app:layout_constraintVertical_bias="0.43"/>
  <TextView
          android:text="Education:"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" android:id="@+id/textView4"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.096"
          app:layout_constraintVertical_bias="0.526"/>
  <TextView
          android:text="Interests:"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" android:id="@+id/textView5"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.1"
          app:layout_constraintVertical_bias="0.629"/>
  <TextView
          android:text="Goals:"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" android:id="@+id/textView6"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.094"
          app:layout_constraintVertical_bias="0.75"/>


</androidx.constraintlayout.widget.ConstraintLayout>
